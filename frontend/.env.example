# Auth.js Configuration
# Generate a random secret: https://generate-secret.vercel.app/32 or `openssl rand -base64 32`
AUTH_SECRET=****
AUTH_TRUST_HOST=true           # Required for deployment behind reverse proxy (Render, etc.)
AUTH_URL=http://localhost:3000 # Set to production URL when deployed (e.g., https://agent-k-frontend.onrender.com)

# The following keys below are automatically created and
# added to your environment when you deploy on Vercel

# Anthropic API Key (required for Claude models)
# Get your API key at: https://console.anthropic.com/
ANTHROPIC_API_KEY=****


# Instructions to create a Vercel Blob Store here: https://vercel.com/docs/vercel-blob
BLOB_READ_WRITE_TOKEN=****

# Instructions to create a PostgreSQL database here: https://vercel.com/docs/postgres
POSTGRES_URL=****


# Instructions to create a Redis store here:
# https://vercel.com/docs/redis
REDIS_URL=****

# Python Backend URL for Agent K (Pydantic AI)
# This is the URL of the Python backend running the Pydantic AI agent
# For local development: http://localhost:9000/agentic_generative_ui/
# For production: Set to your deployed Python backend URL
PYTHON_BACKEND_URL=http://localhost:9000/agentic_generative_ui/
